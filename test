const ss = [
  {
    aa: 123,
  },
  {
    aa: 222,
  },
  {
    aa: 35,
  },
];

console.log(ss.sort(byProperty('aa', '>')));

function byProperty(property, sort) {
    return function (a, b) {
        if (sort === '<') {
            return a[property] > b[property] ? 1 : -1;
        } if (sort === '>') {
            return a[property] > b[property] ? -1 : 1;
        }
    }

}